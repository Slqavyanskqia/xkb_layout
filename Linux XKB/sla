xkb_symbols "sla_CyrlE1" {
  include "sla(sla_base)"
	///////////////////////// Мећусловјанскы (Кирилица Е1) /////////////////////////
	name[Group1]= "(Кирилица Е1)";
	// WEST-EUROPEAN LAYOUT

	// SECOND ROW
    key.type[Group1] = "EIGHT_LEVEL_ALPHABETIC";
    key <AD01> { [           U044A,      U042A,                U044C,    U042C,                   at,             NoSymbol, NoSymbol, NoSymbol ] }; // ъ Ъ ь Ь @
    key <AD02> { [           U0443,      U0423,                U044E,    U042E,                UA64B,                UA64A,    U0479,    U0478 ] }; // у У ю Ю ꙋ Ꙋ ѹ Ѹ
    key <AD03> { [           U0435,      U0415,                U0465,    U0464,                U044D,                U042D,    U0454,    U0404 ] }; // е Е ѥ Ѥ э Э є Є
    key <AD04> { [           U0440,      U0420,                U0306,    U0487,            trademark,           registered, NoSymbol, NoSymbol ] }; // р Р ̆ ҇ ™ ®
    key <AD05> { [           U0442,      U0422,                U047F,    U047E,             NoSymbol,             NoSymbol, NoSymbol, NoSymbol ] }; // т Т ѿ Ѿ
    key <AD06> { [           U0437,      U0417,                U046F,    U046E,                UA641,                UA640,    UA643,    UA642 ] }; // з З ѯ Ѯ ꙁ Ꙁ ꙃ Ꙃ
    key <AD07> { [           U046B,      U046A,                U046D,    U046C,           rightarrow,            leftarrow,    U051B,    U051A ] }; // ѫ Ѫ ѭ Ѭ → ← ԛ Ԛ
    key <AD08> { [           U0438,      U0418,                U0439,    U0419,             NoSymbol,             NoSymbol, NoSymbol, NoSymbol ] }; // и И й Й
